{
  "$schema": "https://json-schema.org/draft-07/schema",
  "type": "object",
  "$ref": "#/definitions/Transfer",
  "definitions": {
    "Transfer": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "timestamp": {
          "type": "string",
          "format": "date-time"
        },
        "accepted": {
          "type": ["boolean", "null"]
        },
        "completed": {
          "type": "boolean"
        },
        "tenant_id_from": {
          "type": "integer"
        },
        "tenant_id_to": {
          "type": "integer"
        },
        "chore_type": {
          "type": "string"
        },
        "week_id": {
          "$ref": "week-id.json#/definitions/WeekID"
        }
      },
      "required": [
        "id",
        "timestamp",
        "completed",
        "tenant_id_from",
        "tenant_id_to",
        "chore_type",
        "week_id"
      ]
    }
  }
}
